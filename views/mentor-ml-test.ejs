<head>
    <link rel="stylesheet" href="/css/tests.css">
</head>

<title>JS Quiz</title>
<body>
    <div class="quizContainer container-fluid well well-lg">
        <div id="quiz1" class="text-center">
			<h3>Mentorship test</h3>
			<center><img class="img-responsive" height="180" width="180" src="/images/ewc.jpg"></center>
			<h4 style="color:  #0dcaf0;position:absolute;left:70%;top:30%;" align="center" ><span id="iTimeShow">Time Remaining: </span><br/><span id='timer' style="font-size:25px;"></span></h4>
			<h2>Machine Learning</h2>
		</div>
		
        <div class="question"></div>
        <ul class="choiceList"></ul>
        <form >
            <button style="background-color:grey; color:#0dcaf0; visibility: hidden;" type="submit" ><a style="text-decoration: none;" id="passed" href="/confirm-mentor"></a></button>

        </form>
        <form action=""></form>
            <button style="background-color:grey; color:#0dcaf0; visibility: hidden;" type="submit" ><a style="text-decoration: none;" id="failed" href="/mentor-fail"></a></button>

        </form>
      
        
        <div class="quizMessage"></div>
        <div class="result"></div>
		
        <button style="visibility: visible;" class="nextButton">Next Question</button>
    </div>

    <script src="/js/test/ml-test.js"></script>